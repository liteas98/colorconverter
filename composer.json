{
    "name": "liteas98/colorconverter",
    "description": "My awesome package",
    "keywords": [
        "liteas98",
        "colorconverter"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Mostafa Eid",
            "email": "lost.mostafa92@gmail.com"
        }
    ],
    "require": {
        "php": "*",
        "illuminate/support": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Liteas98\\Colorconverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liteas98\\Colorconverter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liteas98\\Colorconverter\\ColorconverterServiceProvider"
            ],
            "aliases": {
                "Colorcodeconverter": "Liteas98\\Colorconverter\\ColorconverterFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
